<%- include('partials/header') %>
    <h1 class='pt-3'>Home</h1>
    <p><%= homeStartingContent %></p>
    <% console.log(posts) %>
    <% posts.forEach(function(post) { %>
    <div class="card">
    <div class="card-body">
        <h1><%= post.title %></h1>
        <% var truncContent = post.content.slice(0, 99) + " ... "; %>
        <p><%= truncContent %><a href="/posts/<%= post.title %>">Read More</a></p>
    </div>
    </div>
    <% }) %>
<%- include('partials/footer') %>